{
  "name": "meow-ui-vite",
  "version": "1.1.2",
  "description": "Ui for meow component",
  "homepage": "https://meow-ui.vercel.app/",
  "keywords": [
    "UI component, Vue3, Vite, Monorepo"
  ],
  "author": "CatWatermelon",
  "license": "MIT",
  "main": "./dist/meow-ui.mjs",
  "module": "./dist/meow-ui.mjs",
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "package.json",
    "index.html"
  ],
  "exports": {
    ".": {
      "import": "./dist/meow-ui.mjs",
      "require": "./dist/meow-ui.umd.js"
    },
    "./dist/assets/entry.css": {
      "import": "./dist/assets/entry.css",
      "require": "./dist/assets/entry.css"
    }
  },
  "scripts": {
    "test": "vitest",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage",
    "lint": "eslint --fix --ext .ts,.vue ./src",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.vue\"",
    "dev": "vite",
    "build": "vite build"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@iconify-json/ic": "1.1.4",
    "@types/node": "^20.5.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "@vitejs/plugin-vue": "3.0.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vitest/coverage-istanbul": "^0.34.2",
    "@vitest/coverage-v8": "^0.34.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/test-utils": "^2.4.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.47.0",
    "eslint-formatter-pretty": "^5.0.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "happy-dom": "^10.11.0",
    "less": "^4.2.0",
    "prettier": "^3.0.2",
    "terser": "^5.19.2",
    "typescript": "^5.1.6",
    "unocss": "^0.55.1",
    "vite": "3.0.7",
    "vitest": "^0.34.2"
  },
  "dependencies": {
    "vue": "^3.3.4"
  }
}
